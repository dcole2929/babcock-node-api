{
    "name": "babcock-node-api",
    "description": "Node API boilerplate complete with swagger and auth",
    "version": "0.0.1",
    "main": "index.js",
    "license": "UNLICENSED",
    "scripts": {
        "start": "npm run build && node dist",
        "dev": "nodemon src/index.js --exec babel-node",
        "build": "babel src --presets es2015 -d dist --copy-files --source-maps inline",
        "precommit": "lint-staged",
        "lint": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
        "fix": "eslint --ignore-path .gitignore --ignore-pattern \"!**/.*\" --fix .",
        "check": "flow check",
        "test": "jest --testEnvironment=node"
    },
    "lint-staged": {
        "*.js": [
            "eslint --no-ignore --fix",
            "git add --force"
        ],
        "*.json": [
            "prettier --write",
            "git add --force"
        ]
    },
    "_moduleAliases": {
        "~": "."
    },
    "dependencies": {
        "await-to-js": "^2.1.1",
        "bcrypt": "^4.0.1",
        "bluebird": "^3.7.0",
        "body-parser": "^1.19.0",
        "cookie-parser": "^1.4.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-openapi-validator": "^3.10.0",
        "jest": "^24.9.0",
        "js-yaml": "^3.13.1",
        "jsonwebtoken": "^8.5.1",
        "knex": "^0.19.5",
        "module-alias": "^2.2.2",
        "objection": "^1.6.11",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "pg": "^7.18.2",
        "swagger-jsdoc": "^3.5.0",
        "swagger-ui-express": "^4.1.3"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-export-extensions": "^6.22.0",
        "babel-plugin-transform-flow-strip-types": "^6.22.0",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^6.5.1",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-flowtype": "^4.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-prettier": "^3.1.1",
        "lint-staged": "^9.4.2",
        "nodemon": "^2.0.2",
        "prettier": "^1.18.2"
    }
}
